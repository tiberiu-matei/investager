{
    "name": "investager",
    "version": "0.1.0",
    "license": "MIT",
    "scripts": {
        "createlib": "npx nx g @nrwl/js:lib",
        "start": "npx nx run-many --parallel --target=serve --projects=ui",
        "generate": "npx prisma generate --schema=libs/db/prisma/schema.prisma",
        "dbpush": "npx prisma db push --schema=libs/db/prisma/schema.prisma",
        "migrate": "npx prisma migrate dev --schema=libs/db/prisma/schema.prisma",
        "remove": "npx nx g rm",
        "lint": "npx nx run-many --parallel --target=lint --projects=ui",
        "test": "npx nx run-many --parallel --target=test --projects=ui --code-coverage",
        "build": "npx nx run-many --parallel --target=build --projects=ui"
    },
    "private": true,
